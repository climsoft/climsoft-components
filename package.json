{
  "name": "webcomponents",
  "version": "0.1.0",
  "license": "SEE LICENSE in LICENSE",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:examples": "npx browser-sync start --server --watch --startPath examples --files 'components/ examples/'",
    "build": "ng build",
    "build:components": "ng build --configuration production,webcomponents && node scripts/concat-bundles.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:types:opencdms": "npx openapi-typescript https://api.opencdms.org/climsoft/openapi.json --output src/models/opencdms-api.ts",
    "generate:types:components": "npx openapi-typescript https://opencdms-components-api-jnxkyuyk3q-ew.a.run.app/openapi.json --output src/models/components-api.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.6",
    "@angular/cdk": "^13.2.4",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/elements": "^13.2.4",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "^13.2.6",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@coreui/angular": "^4.0.0-alpha.9",
    "@coreui/coreui": "^4.1.0",
    "@coreui/icons": "^2.1.0",
    "@coreui/icons-angular": "^3.0.0-alpha.4",
    "openapi-typescript-fetch": "^1.1.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "concat": "^1.0.3",
    "fs-extra": "^10.0.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
